{
   "background": {
      "persistent": false,
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19.png",
         "38": "images/icon-38.png"
      },
      "default_title": "Click to disable Copycat"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+1"
         }
      },
      "toggle-copycat": {
         "description": "Open and Close Copycat",
         "suggested_key": {
            "default": "Ctrl+Shift+2"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "scripts/injected/main.css" ],
      "js": [ "scripts/injected/main.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "storage", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.62",
   "web_accessible_resources": [ "scripts/injected/flyout/app.html" ]
}
